    "vue": "^3.2.25",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@capacitor/android": "^3.8.0",
    "@capacitor/cli": "^3.8.0",
    "@capacitor/core": "^3.8.0",
    "@heroicons/vue": "^1.0.6",
    "@types/lodash.merge": "^4.6.7",
    "@types/node": "18.8.0",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@vitejs/plugin-vue": "^3.1.2",
    "@volar/vue-language-plugin-pug": "^1.0.7",
