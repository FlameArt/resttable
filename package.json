{
  "name": "flameresttable",
  "version": "1.0.15",
  "main": "./src/plugin.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/FlameArt/resttable.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "npm run build && vite preview",
    "install/update": "npm i && npm update --save",
    "push and publish": "git push && npm --no-git-tag-version version patch && npm publish",
    "android": "npm run build && npx cap add android && npx cap sync && npx cap open android"
  },
  "dependencies": {
    "@vuepic/vue-datepicker": "^7.2.0",
    "flamerest": "^1.0.92",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.7",
    "pug": "github:FlameArt/pug-tailwind-only",
    "pug-lexer": "github:FlameArt/pug-lexer-only",
    "sass": "^1.69.5",
    "vue": "^3.3.7",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@heroicons/vue": "^2.0.18",
    "@types/lodash-es": "^4.17.10",
    "@types/lodash.merge": "^4.6.8",
    "@types/node": "20.8.9",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vitejs/plugin-vue": "^4.4.0",
    "@volar/vue-language-plugin-pug": "^1.6.5",
    "@vue/compiler-sfc": "^3.3.7",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.52.0",
    "eslint-plugin-vue": "^8.7.1",
    "postcss": "^8.4.31",
    "postcss-import": "^14.1.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vue-tsc": "^1.8.22",
    "vue-universal-modal": "^1.1.4"
  },
  "rules": {}
}
