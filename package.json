{
  "name": "flameresttable",
  "version": "1.0.1",
  "main": "./src/plugin.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/FlameArt/resttable.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "npm run build && vite preview",
    "install/update": "npm i && npm update --save",
    "push and publish": "git push && npm --no-git-tag-version version patch && npm publish",
    "android": "npm run build && npx cap add android && npx cap sync && npx cap open android"
  },
  "dependencies": {
    "@vuepic/vue-datepicker": "^3.6.8",
    "flamerest": "^1.0.63",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.32",
    "pug": "github:FlameArt/pug-tailwind-only",
    "pug-lexer": "github:FlameArt/pug-lexer-only",
    "sass": "^1.58.3",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@capacitor/android": "^3.9.0",
    "@capacitor/cli": "^3.9.0",
    "@capacitor/core": "^3.9.0",
    "@heroicons/vue": "^2.0.16",
    "@types/lodash-es": "^4.17.6",
    "@types/lodash.merge": "^4.6.7",
    "@types/node": "18.8.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@volar/vue-language-plugin-pug": "^1.2.0",
    "@vue/compiler-sfc": "^3.2.45",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^8.7.1",
    "postcss": "^8.4.21",
    "postcss-import": "^14.1.0",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vue-tsc": "^1.2.0",
    "vue-universal-modal": "^1.1.4"
  },
  "rules": {}
}
